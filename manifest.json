{
   "background": {
      "persistent": true,
      "scripts": [ "main.js",
                    "twisc/background.js" ]
   },
   "browser_action": {
      "default_icon": {
            "16": "twisc/images/get_started16.png",
            "32": "twisc/images/get_started32.png",
            "48": "twisc/images/get_started48.png",
            "128": "twisc/images/get_started128.png"
      },
      "default_title": "Disinformation Extension"
   },
   "default_locale": "en",
   "description": "__MSG_appDescription__",
   "icons": {
        "16": "twisc/images/get_started16.png",
        "32": "twisc/images/get_started32.png",
        "48": "twisc/images/get_started48.png",
        "128": "twisc/images/get_started128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Disinformation Extension",
   "permissions": [ "webRequest", "webRequestBlocking", "tabs", "downloads", "storage", "desktopCapture", "notifications", "idle", "\u003Call_urls>",
                    "activeTab", "declarativeContent", "clipboardRead", "clipboardWrite" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.3.9",
    "web_accessible_resources": [
        "twisc/images/check-t-green.png",
        "twisc/images/check-t-opinion.png",
        "twisc/images/check-t-red.png",
        "twisc/images/check-t-unknown.png",
        "twisc/images/check-t-notrated.png",
        "twisc/images/check-t-error.svg",
        "twisc/images/check-t-loading.gif",
        "twisc/images/green-check-mark.jpg",
        "twisc/images/red-warning.jpg",
        "twisc/images/big-questionmark.png",
        "twisc/images/twisc.png",
        "twisc/images/fb-post.png",
        "twisc/images/fb-message.jpg",
        "twisc/images/copy.png",
        "twisc/images/send.png",
        "twisc/popover-template.html"
    ],
    "options_page": "twisc/options.html",
     "content_scripts": [
       {
         "matches": ["*://*.facebook.com/*",
                     "*://*.messenger.com/*"],
         "all_frames": true,
         "css": ["twisc/styles.css"],
         "js": ["twisc/common-resources.js",
                "twisc/common-settings.js",
                "twisc/content.js",
                "twisc/content-popover.js",
                "twisc/content-add-widgets.js",
                "twisc/content-hashes.js",
                "twisc/content-evaluation.js",
                "twisc/content-evaluator-newsguard.js",
                "twisc/content-evaluator-cofacts.js",
                "twisc/content-fb.js",
                "twisc/content-handler.js",
                "twisc/content-handler-fb-old-feed-messagebox.js",
                "twisc/content-handler-fb-new-feed-messagebox.js",
                "twisc/content-handler-fb-old-feed-post.js",
                "twisc/content-handler-fb-new-feed-post.js",
                "twisc/content-handler-fb-messenger.js"]
       }
    ]
}
